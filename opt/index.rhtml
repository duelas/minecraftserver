<% open('ngrok.log') do |f| %>
	<% f.lines.select { |line| line.include?("URL:") }.each do |line| %>
		Server available at: <%= line.match(/tcp:\/\/(.+:[0-9]+) /)[1] %>
		<br />
	<% end %>
<% end %>
World Map available at: <%= line.read %>	
<br />
<% open('transfer.txt') do |list| %>
	<% list.tail(60).each_line do |e| %>
		<%= e.read %>
	<% end %>
<% end %>
