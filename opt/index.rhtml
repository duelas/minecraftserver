<% open('ngrok.log') do |f| %>
  <% f.lines.select { |line| line.include?("URL:") }.each do |line| %>
    Server available at: <%= line.match(/tcp:\/\/(.+:[0-9]+) /)[1] %>
    <br />
  <% end %>
<% end %>
<% open('transfer.txt') do |map| %>
    World Map available at: <%= puts map %>	
    <br />
<% end %>
